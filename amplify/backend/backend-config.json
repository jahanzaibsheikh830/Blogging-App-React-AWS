{
  "api": {
    "blog": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 30,
              "description": ""
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  },
  "function": {
    "blogsfunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "Blog",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "storage",
          "resourceName": "Post",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "storage",
          "resourceName": "Comment",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "storage": {
    "Blog": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "Post": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "Comment": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  }
}